{
    "name": "quick",
    "version": "2.5.4",
    "author": "Ortus Solutions",
    "location": "coldbox-modules/quick#v2.5.4",
    "homepage": "https://github.com/coldbox-modules/quick",
    "documentation": "https://github.com/coldbox-modules/quick",
    "repository": {
        "type": "git",
        "URL": "https://github.com/coldbox-modules/quick"
    },
    "bugs": "https://github.com/coldbox-modules/quick/issues",
    "slug": "quick",
    "shortDescription": "A ColdBox ORM Engine",
    "description": "A ColdBox ORM Engine",
    "scripts": {
        "format": "cfformat run dsl/**/*.cfc,extras/**/*.cfc,interceptors/**/*.cfc,models/**/*.cfc,tests/specs/**/*.cfc --overwrite",
        "format:check": "cfformat check dsl/**/*.cfc,extras/**/*.cfc,interceptors/**/*.cfc,models/**/*.cfc,tests/specs/**/*.cfc",
        "format:watch": "cfformat watch dsl/**/*.cfc,extras/**/*.cfc,interceptors/**/*.cfc,models/**/*.cfc,tests/specs/**/*.cfc",
        "postVersion": "package set location='coldbox-modules/quick#v`package version`'",
        "generateAPIDocs": "rm .tmp --recurse --force && docbox generate mapping=quick excludes=test|module|QuickCollection strategy-outputDir=.tmp/apidocs strategy-projectTitle=Quick"
    },
    "type": "modules",
    "dependencies": {
        "qb": "^7.3.0",
        "str": "^1.0.0",
        "testbox": "^3.1.0+339",
        "mementifier": "github:coldbox-modules/mementifier#trusted_getters_and_better_mappers"
    },
    "devDependencies": {
        "coldbox": "be",
        "testbox": "^3.1.0+339",
        "cfcollection": "^3.6.1"
    },
    "installPaths": {
        "testbox": "testbox/",
        "coldbox": "tests/resources/app/coldbox/",
        "qb": "modules/qb/",
        "str": "modules/str/",
        "cbvalidation": "modules/cbvalidation/",
        "cfcollection": "modules/cfcollection/",
        "mementifier": "modules/mementifier/"
    },
    "testbox": {
        "reporter": "json",
        "runner": "http://127.0.0.1:52482/tests/runner.cfm",
        "verbose": "false",
        "watchDelay": "1000"
    },
    "ignore": [
        "**/.*",
        "test",
        "tests",
        ".engine",
        ".travis.yml",
        ".gitignore",
        "server.json"
    ]
}
